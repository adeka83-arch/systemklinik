#!/bin/bash

# Script to remove voucher-related components
echo "üßπ Cleaning up voucher components..."

# List of voucher components to remove
voucher_components=(
    "VoucherCard.tsx"
    "VoucherFixShowcase.tsx"
    "VoucherImageDemo.tsx"
    "VoucherReminder.tsx"
    "VoucherReminderList.tsx"
    "VoucherSelector.tsx"
    "VoucherStatusList.tsx"
    "VoucherStatusListFixed.tsx"
    "VoucherUsage.tsx"
    "VoucherUsageDemo.tsx"
    "VoucherUsageHistory.tsx"
)

# Remove each component
for component in "${voucher_components[@]}"; do
    if [ -f "/components/$component" ]; then
        echo "üóëÔ∏è Removing $component"
        rm "/components/$component"
    else
        echo "‚ö†Ô∏è $component not found"
    fi
done

echo "‚úÖ Voucher component cleanup completed"